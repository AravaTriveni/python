class Depth:
    def __init__(self,data):
        self.val=data
        self.left=left
        self.right=right
    def maxDepth(root):
        if(root==None):
            return 0
        lh=self.maxDepth(root.left)
        rh=self.maxDepth(root.right)
        return 1+max(lh,rh)
root=list(map(int,input().split()))
print(maxDepth(root))
