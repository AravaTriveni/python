#floor: largest element in the array such that [element<=x]
def FindFloor(arr,x):
    low=0
    high=len(arr)-1
    ans=-1
    while(low<=high):
        mid=(low+high)//2
        if(arr[mid]<=x):
            ans=mid
            low=mid+1
        else:
            high=mid-1
    return ans
arr=list(map(int,input().split()))
x=int(input())
print(FindFloor(arr,x))
